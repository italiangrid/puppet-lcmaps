<%-
@pools.each do | pool |
  @vo_name=pool['vo']
  @group=pool['group']
  @role=pool['role']
  if @role == nil
    @role1 = "/Role=NULL"
    @role2 = ""
  else
    @role1 = "/Role=#{@role}"
    @role2 = @role1
  end
  @capability=pool['capability']
  if @capability == nil
    @capability1 = "/Capability=NULL"
    @capability2 = ""
  else
    @capability1 = "/Capability=#{@capability}"
    @capability2 = @capability1
  end
-%>
"/<%=@vo_name%><%=@role1%><%=@capability1%>" <%=@group%>
"/<%=@vo_name%><%=@role2%><%=@capability2%>" <%=@group%>
<%- 
end
-%>